{
  "name": "electron-greenscreen",
  "version": "0.0.2",
  "description": "Makes a Window to display the HTML Page.",
  "main": "main.js",
  "scripts": {
    "start": ".\\node_modules\\.bin\\electron .",
    "build": "electron-packager . GreenScreen --icon icon.ico",
    "snyk-protect": "snyk protect",
