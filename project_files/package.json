  },
  "build": {
    "appId": "GreenScreen",
    "directories": {
      "output": "packages"
    }
  },
  "author": "Noah Halstead <nhalstead00@gmail.com>",
  "dependencies": {
    "electron": "^11.5.0",
    "nodemon": "^2.0.3",
    "snyk": "^1.305.1"
  },
  "devDependencies": {
    "electron-packager": "^10.1.0"
  },
  "snyk": true
}
